<div class="container">
<div class="row">
<div class="span6">
  		<div class="slo">欢迎来到 J 公司签到系统，今天公司以你为荣，明天公司以你为傲</h1>
		</div>
		</div>
<div class="span6 offset8">
<h2>Sign up</h2>
<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>

  <div><%= f.label :Username %>
  <%= f.text_field :username, :autofocus => true %></div></br>
 
  <div><%= f.label :Email %>
  <%= f.email_field :email %></div>
 
  
  <div><%= f.label :Phonenumber %>
  <%= f.text_field :phonenumber %></div>

  <div><%= f.label :Password %>
  <%= f.password_field :password %></div>

  <div><%= f.label :Password_confirmation %>
  <%= f.password_field :password_confirmation %></div>

  <div class="submit"><%= f.submit "Sign up" , class: "btn btn-large btn-block  btn-success"%></div>
<% end %>

</div>
</div>
</div>
