<div class="container">
<div class="row">
<div class="span6">
  <div class="slogan">
  		<h1 class="slogan-title">员工签到系统</h1>
		<p class="slogan-description">带着轻松，愉快的心情上班，为你们的准时加薪</p>
		</div>
		</div>
<div class="span6 offset8">
<h2>Sign up</h2>
<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>

  <div><%= f.label :username %><br />
  <%= f.text_field :username, :autofocus => true %></div>
 
  <div><%= f.label :email %><br />
  <%= f.email_field :email %></div>
 
  
  <div><%= f.label :phonenumber %><br/>
  <%= f.text_field :phonenumber %></div>

  <div><%= f.label :password %><br />
  <%= f.password_field :password %></div>

  <div><%= f.label :password_confirmation %><br />
  <%= f.password_field :password_confirmation %></div>

  <div><%= f.submit "Sign up" , class: "btn btn-large btn-primary"%></div>
<% end %>

</div>
</div>
</div>
